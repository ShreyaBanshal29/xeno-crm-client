{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/manifest.json",
            "dest": "/manifest.json"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}